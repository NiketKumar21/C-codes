#include <stdio.h>

int main() {
	
	int t;
	scanf("%d",&t);
	int i;
	for(i=1;i<=t;i=i+1)
	{
	    int a,b,c;
	    scanf("%d %d %d",&a,&b,&c);
	    
	    if(a>50&&a>b&&a>c)
	    {
	        printf("A\n");
	    }
	    else
	    {
	        if(b>50&&b>a&&b>c)
	        {
	            printf("B\n");
	        }
	        else
	        {
	            if(c>50&&c>a&&c>b)
	            {
	                printf("C\n");
	            }
	            else
	            {
	                printf("NOTA\n");
	            }
	        }
	    }
	}
}
